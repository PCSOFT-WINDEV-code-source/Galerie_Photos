  procedures :
   -
     name : bDoitActiverSSL
     internal_properties : CAAAAAgAAAC0TaGuYi2PqOQmiC1YmXJbMUTnjPWO9mhkQd3KIBH1rSc/lp1PMClvZtdu4BMhj7h3XT0qh3VDLEy3sw==
     procedure_id : 2294434326813337978
     type_code : 14
     code : |1+
      // Résumé : Indique si on doit activer le mode SSL
      PROCEDURE bDoitActiverSSL()
      
      // si on ne veut pas activer le SSL
      si COL_GPU_CNX.gbActiveSSL=faux ALORS
      	renvoyer faux
      FIN
      
      // Non : si on est déjà en SSL
      EXTERNE location
      SI location.protocol = "https" ALORS
      	RENVOYER Faux
      FIN
      
      // NON : Si la configuration ne le demande pas
      // @@@ Toujours pour le moment
      RENVOYER Vrai
     type : 458752
     role : N
   -
     name : bConnexionEnCours
     procedure_id : 2294434502908807617
     type_code : 14
     code : |1+
      // Résumé : Savoir si un utilisateur est actuellement connecté
      PROCEDURE bConnexionEnCours()
      
      // récupération de l'utilisateur connecté
      sLogin est une chaine unicode=gpwRecupInfoUtilisateur(gpwInfoLogin)
      
      // si aucun utilisateur n'est encore connecté
      SI sLogin="" _ou_ sLogin=gpwVisiteur ALORS
      	renvoyer faux
      FIN
      
      // si un utilisateur est actuellement connecté
      renvoyer vrai
     type : 458752
   -
     name : AdapteLibelleConnexion
     procedure_id : 2294443002705919111
     type_code : 14
     code : |1+
      // Résumé : Adapte le libellé du bouton Connexion
      PROCEDURE AdapteLibelleConnexion()
      
      // si un utilisateur est actuellement connecté
      SI bConnexionEnCours() ALORS
      	// propose la déconnexion
      	LIEN_Connexion..Libellé=<§$0000§>
      	
      	// si aucun utilisateur n'est encore connecté
      SINON
      	// propose la connexion
      	LIEN_Connexion..Libellé=<§$0001§>
      FIN
      
     type : 458752
